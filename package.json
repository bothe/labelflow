{
  "private": true,
  "name": "labelflow",
  "version": "0.0.1",
  "repository": "git@github.com:Labelflow/labelflow.git",
  "scripts": {
    "lint": "eslint '{cypress,typescript}/**/*.{ts,tsx}'",
    "lint:all": "eslint '{cypress,typescript}/**/*.{ts,tsx}'",
    "test": "jest --runInBand",
    "test:all": "jest --runInBand",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev:web": "cd ./typescript/web && yarn dev",
    "build:web": "cd ./typescript/web && yarn build",
    "start:web": "cd ./typescript/web && yarn start",
    "bundlewatch:web": "cd ./typescript/web && yarn build && yarn bundlewatch",
    "codegen": "graphql-codegen --config ./codegen.yml",
    "codegen:ci": "graphql-codegen --config ./codegen.yml && git diff --exit-code",
    "storybook": "start-storybook -s ./typescript/web/public -p 6006",
    "storybook:all": "start-storybook -s ./typescript/web/public -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic",
    "release": "release-it",
    "release:update": "cd ./typescript/release && yarn update"
  },
  "workspaces": [
    "typescript/*"
  ],
  "devDependencies": {
    "@apollo/client": "^3.3.21",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.7",
    "@cypress/webpack-preprocessor": "^5.9.1",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/schema-ast": "^1.18.2",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-resolvers": "^1.19.1",
    "@storybook/addon-actions": "6.3.0",
    "@storybook/addons": "6.3.0",
    "@storybook/builder-webpack5": "next",
    "@storybook/manager-webpack5": "next",
    "@storybook/react": "6.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "auto-changelog": "^2.3.0",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.8.2",
    "cypress": "7.3.0",
    "dexie": "^3.0.3",
    "eslint": "7.26.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "fake-indexeddb": "^3.1.2",
    "graphql": "15.5.1",
    "graphql-tag": "2.12.4",
    "jest": "26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-transform-graphql": "^2.1.0",
    "node-polyfill-webpack-plugin": "^1.1.2",
    "prettier": "2.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "release-it": "^14.11.5",
    "service-worker-mock": "patch:service-worker-mock@2.0.5#./service-worker-mock.patch",
    "ts-jest": "26.5.6",
    "ts-loader": "^9.2.3",
    "ts-node": "9.1.1",
    "typescript": "4.3.5",
    "webpack": "5.38.1"
  }
}
